<%- include('./partials/header.ejs') %> 
<div class="page-container">
    <a href="/gallery" class="return"><i class="fa-solid fa-arrow-left"></i> Retour</a>
    <div class="oneElement">
        

        <h1> <%= element.nom %> </h1>
        <img src="../public/images/<%= element.file %>">

        <p><%= element.description  %> </p>

        <div class="workshop">
            <iframe width="1120" height="630" src="<%= element.youtube %>" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            <% for (const workshop of element.workshops) { %>
                <img src="../public/images/<%= workshop %>">
            <% } %>
        </div>
    </div>
</div>
<%- include('./partials/footer.ejs') %> 